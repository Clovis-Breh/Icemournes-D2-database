      "896224899": {
         "name": "Relentless Tracker",
         "item_name": "Foetracer",
         "description": [
            {
               "lineText": [
                  {
                     "text": "Marks Enemies for 5 seconds after hovering reticle over them. Persists briefly through walls."
                  }
               ]
            },
            {
               "lineClass": "spacer_class"
            },
            {
               "lineText": [
                  {
                     "text": "If Enemy is below 30% HP: Grants a 1% Damage Increase per percent below 30%"
                  }
               ]
            },
            {
               "lineText": [
                  {
                     "text": "(Eg: 10% HP left -> 20% Damage Increase)"
                  }
               ]
            },
            {
               "lineClass": "spacer_class"
            },
            {
               "lineText": [
                  {
                     "text": "Damage Increase applies to both abilities and weapons."
                  }
               ]
            }
         ],
         "last_edit": "2021.9.4-18.48"
      },
      "1163283805": {
         "name": "Misdirection",
         "item_name": "Gemini Jester",
         "description": [
            {
               "lineText": [
                  {
                     "text": "On Dodge:"
                  }
               ]
            },
            {
               "lineText": [
                  {
                     "text": "Applies Misdirection to enemies within 15 metres."
                  }
               ]
            },
            {
               "lineClass": "spacer_class"
            },
            {
               "lineText": [
                  {
                     "text": "[PVE] Misdirection disorients minors as well as elites for 5 seconds."
                  }
               ]
            },
            {
               "lineText": [
                  {
                     "text": "[PVP] Misdirection warps audio and removes radar for 5 seconds, as well as briefly blinding."
                  }
               ]
            },
            {
               "lineClass": "spacer_class"
            },
            {
               "lineText": [
                  {
                     "text": "Misdirection also deals 1 damage."
                  }
               ]
            }
         ],
         "last_edit": "2021.9.4-18.54"
      },
      "1219761634": {
         "name": "Parting Gift",
         "item_name": "The Bombardiers",
         "description": [
            {
               "lineText": [
                  {
                     "text": "On Dodge:"
                  }
               ]
            },
            {
               "lineText": [
                  {
                     "text": "Drops a Parting Gift that explodes and deals 120 Damage over 7.5 metres with a 1(?) second fuse."
                  }
               ]
            },
            {
               "lineClass": "spacer_class"
            },
            {
               "lineText": [
                  {
                     "text": "Parting Gift has an additional effect based on Subclass:"
                  }
